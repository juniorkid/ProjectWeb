gcc core.c enc.c common.c misc.c policy_lang.c -o cpabe_enc -I/usr/include/glib-2.0 -I/usr/lib/glib-2.0/include -I/usr/local/include/pbc -I/usr/local/ssl/include -I/usr/local/openssl/include -I/usr/local/include -lglib-2.0 -Wl,--start-group /usr/local/lib/libpbc.a /usr/local/ssl/lib/libssl.a /usr/local/ssl/lib/libcrypto.a /usr/local/openssl/lib/libssl.a /usr/local/openssl/lib/libcrypto.a /usr/local/lib/libgmp.a -Wl,--end-group



