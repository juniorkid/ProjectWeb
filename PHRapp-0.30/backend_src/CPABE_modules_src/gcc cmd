gcc -I/usr/include/glib-2.0 -I/usr/lib/glib-2.0/include -lglib-2.0 -I/usr/local/include/pbc /usr/local/lib/libpbc.a -I/usr/local/ssl/include /usr/local/ssl/lib/libssl.a /usr/local/ssl/lib/libcrypto.a -I/usr/local/openssl/include /usr/local/openssl/lib/libssl.a /usr/local/openssl/lib/libcrypto.a -I/usr/local/include /usr/local/lib/libgmp.a core.c setup.c common.c misc.c policy_lang.c -o setup



setup (Work)
gcc core.c setup.c common.c misc.c policy_lang.c -o cpabe_setup -I/usr/include/glib-2.0 -I/usr/lib/glib-2.0/include -I/usr/local/include/pbc -I/usr/local/ssl/include -I/usr/local/openssl/include -I/usr/local/include -lglib-2.0 -Wl,--start-group /usr/local/lib/libpbc.a /usr/local/ssl/lib/libssl.a /usr/local/ssl/lib/libcrypto.a /usr/local/openssl/lib/libssl.a /usr/local/openssl/lib/libcrypto.a /usr/local/lib/libgmp.a -Wl,--end-group


keygen (Work)
gcc core.c keygen.c common.c misc.c policy_lang.c -o cpabe_keygen -I/usr/include/glib-2.0 -I/usr/lib/glib-2.0/include -I/usr/local/include/pbc -I/usr/local/ssl/include -I/usr/local/openssl/include -I/usr/local/include -lglib-2.0 -Wl,--start-group /usr/local/lib/libpbc.a /usr/local/ssl/lib/libssl.a /usr/local/ssl/lib/libcrypto.a /usr/local/openssl/lib/libssl.a /usr/local/openssl/lib/libcrypto.a /usr/local/lib/libgmp.a -Wl,--end-group


